   "source": [
    "**Trailing zeros**\n",
    "How many trailing zeros in $n!$?"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 26,
   "id": "87b12bda",
   "metadata": {},
   "outputs": [],
   "source": [
    "def trailing_zeros(n):\n",
    "    zeros = sum(n // 5**k for k in range(1, n) if 5**k <= n)\n",
    "    return zeros"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "id": "8ec3f92d",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "The number of trailing zeros in  2340 ! is: 582\n"
     ]
    }
   ],
   "source": [
    "n=100\n",
    "zeros=trailing_zeros(n)\n",
    "print(\"The number of trailing zeros in \",n,\"! is:\",zeros)"
   ]
  }
